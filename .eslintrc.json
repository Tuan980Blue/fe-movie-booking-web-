{
  "root": true,
  "extends": [
    "react-app",
    "react-app/jest",
    "plugin:import/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "prettier"
  ],
  "plugins": ["import"],
  "rules": {
    "import/no-unresolved": ["error", { "ignore": ["^@/"] }],
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"],
        "extensions": [".js", ".jsx", ".json"]
      }
    }
  }
}


